<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dsep.entity.dsepmeta.ConvFactorValue" dynamic-update="true" table="DSEP_O_CONV_FACTOR_2013">
        <id name="id" type="java.lang.String" >
            <column name="ID" length="32"/>
            <generator class="guid" />
        </id>
        <property name="indexId" type="java.lang.String" >
            <column name="INDEX_ID" length="32"/>
        </property>
         <property name="discId" type="java.lang.String" >
            <column name="DISC_ID" />
        </property>
        <property name="content" type="java.lang.String" >
            <column name="CONTENT" length="200"/>
        </property>
        <property name="avgValue" type="double" >
            <column name="AVG_VALUE" />
        </property>
    </class>
</hibernate-mapping>

