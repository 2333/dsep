<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dsep.entity.dsepmeta.DataCalculateConfig" dynamic-update="true" table="DSEP_O_DATA_CAL_CONFIG_2013">
        <id name="id" type="java.lang.String" >
            <column name="ID" length="32"/>
            <generator class="guid" />
        </id>
        <property name="discId" type="java.lang.String" >
            <column name="DISC_ID" length="4"/>
        </property>
        <property name="discName" type="java.lang.String" >
            <column name="DISC_NAME" length="100"/>
        </property>
        <property name="userId" type="java.lang.String" >
            <column name="USER_ID" length="32"/>
        </property>
        <property name="calStatus" type="java.lang.String" >
            <column name="CAL_STATUS" length="1"/>
        </property>
        <property name="calTime" type="java.util.Date" >
            <column name="CAL_TIME" />
        </property>
        <property name="distance" type="java.lang.String" >
            <column name="DISTANCE" length="6"/>
        </property>
    </class>
</hibernate-mapping>

