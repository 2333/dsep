<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">	
<hibernate-mapping>
    <class name="com.dsep.entity.dsepmeta.IndexScore" dynamic-update="true" table="DSEP_O_CAL_INDEXSCORE_2013">
        <id name="id" type="java.lang.String" >
            <column name="ID" length="32"/>
            <generator class="guid" />
        </id>
        <property name="unitId" type="java.lang.String" >
            <column name="UNIT_ID" length="5"/>
        </property>
        <property name="discId" type="java.lang.String" >
            <column name="DISC_ID" length="4"/>
        </property>
       <property name="indexId" type="java.lang.String" >
            <column name="INDEX_ID" length="32"/>
        </property>
        <property name="indexName" type="java.lang.String" >
            <column name="INDEX_NAME" length="32"/>
        </property>
        <property name="indexLevel" type="int" >
            <column name="INDEX_LEVEL" length="4"/>
        </property>
        <property name="score" type="double" >
            <column name="SCORE" />
        </property>
    </class>
</hibernate-mapping>