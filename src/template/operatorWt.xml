<?xml version="1.0" encoding="UTF-8"?>
<!-- top代表该计算符来自栈顶，out代表该计算符来自栈外 -->
<root>
  <weight>
    <c content="+" location="top" weight="3"></c>
	<c content="+" location="out" weight="2"></c>
	<c content="-" location="top" weight="3"></c>
	<c content="-" location="out" weight="2"></c>
	<c content="*" location="top" weight="6"></c>
	<c content="*" location="out" weight="5"></c>
	<c content="/" location="top" weight="6"></c>
	<c content="/" location="out" weight="5"></c>
	<c content="(" location="top" weight="1"></c>
	<c content="(" location="out" weight="9"></c>
	<c content=")" location="top" weight="9"></c>
	<c content=")" location="out" weight="1"></c>
	<c content="#" location="top" weight="0"></c>
	<c content="#" location="out" weight="0"></c>
  </weight>
</root>

