<root>
	<!-- 获奖等级字段名称  begin-->
	<award>
		<field name="HJ_LEVEL"/>
		<field name="HJDJ"/>
		<field name="AWARD_GRADE"/>
	</award>
	<!-- 获奖等级字段名称  end -->
	<!-- 学校学科排名占百分比字段名称  begin -->
	<sort>
		<field name="HJ_UNIT_NUM"/>
		<field name="HJ_DISC_NUM"/>
		<field name="CYDWS"/>
		<field name="BDWXKS"/>
		<field name="DEPT_AMOUT"/>
		<field name="DISCIPLINE_AMOUT"/>
		<field name="BRPX"/>
	</sort>
	<!-- 学校学科排名占百分比字段名称  end -->
</root>