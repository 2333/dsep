<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">	
<hibernate-mapping>
    <class name="com.dsep.entity.dsepmeta.CalResult" dynamic-update="true" table="DSEP_O_CAL_TOTALRESULT_2013">
        <id name="id" type="java.lang.String" >
            <column name="ID" length="32"/>
            <generator class="guid" />
        </id>
        <property name="unitId" type="java.lang.String" >
            <column name="UNIT_ID" length="5"/>
        </property>
        <property name="discId" type="java.lang.String" >
            <column name="DISC_ID" length="4"/>
        </property>
        <property name="score" type="java.lang.Double" >
            <column name="SCORE" length="10"/>
        </property>
         <property name="rank" type="int">
        	<column name="RANK" length="10"></column>
        </property>
        <property name="cluClass" type="java.lang.String" >
            <column name="CLU_CLASS"  length="10"/>
        </property>
        
    </class>
</hibernate-mapping>