<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dsep.entity.project.ItemExecute" table="DSEP_P_ITEMEXECUTE_2013">
		<id name="id" type="java.lang.String">
			<column name="ID"/>
			<generator class="guid"/>
		</id>
		<property name="executeStatus" type="java.lang.String">
			<column name="EXECUTE_STATUS"></column>
		</property>
		<property name="txtRecord" type="java.lang.String">
			<column name="TXT_RECORD"></column>
		</property>
		<property name="score" type="java.lang.String">
			<column name="SCORE"></column>
		</property>
		<many-to-one name="passItem" class="com.dsep.entity.project.PassItem" 
            column="ITEM_ID" >
        </many-to-one>
	</class>
</hibernate-mapping>