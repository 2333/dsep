<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dsep.entity.expert.EvalPaper" table="DSEP_X_EVAL_PAPER_2014">
        <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="guid" />
        </id>
        <property name="expertTypeId" type="java.lang.String">
            <column name="EXPERT_TYPE_ID" length="32" />
        </property>
        <!--<property name="disciplineTypeId" type="java.lang.String">
        	 DISC_CAT_ID表示JSJ等
            <column name="DISC_CAT_ID" length="32" />
        </property>
         -->
        <property name="discCatId" type="java.lang.String">
            <column name="DISC_CAT_ID" length="50" />
        </property>
        <property name="isMeta" type="java.lang.Boolean">
            <column name="IS_META" />
        </property>
        <set
        	name="evalQuestions"
        	table="DSEP_X_PAPER_QUES_2014"
        	lazy="false"
        	inverse="false"
        	cascade="all">
        	<key 
        		column="PAPER_ID"></key>
        	<many-to-many 
        		class="com.dsep.entity.expert.EvalQuestion" 
        		column="QUESTION_ID">
        	</many-to-many>	
        </set>
        <many-to-one name="evalBatch" 
        class="com.dsep.entity.expert.EvalBatch" 
        cascade="none"
        outer-join="auto"
        insert="true" 
        access="property"
        column="BATCH_ID"
        not-null="true">
   		</many-to-one>
    </class>
</hibernate-mapping>
