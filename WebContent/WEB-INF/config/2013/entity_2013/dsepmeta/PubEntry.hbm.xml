<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dsep.entity.dsepmeta.PubEntry" table="DSEP_D_PUB_ENTRY_2013">
		<id name="publibId" type="java.lang.String">
            <column name="PUBLIB_ID" length="32" />
        </id>
		<property name="publibChsName" type="java.lang.String">
			<column name="PUBLIB_CHS_NAME" length="100"/>
		</property>
		<property name="checkUser" type="java.lang.String">
			<column name="CHECK_USER" length="32"/>
		</property>
		
		<property name="checkDate" type="java.util.Date">
			<column name="CHECK_DATE" />
		</property>
		<property name="isChecked" type="java.lang.Character">
			<column name="IS_CHECKED" />
		</property>
	</class>
</hibernate-mapping>
